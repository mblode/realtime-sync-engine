{
	"name": "realtime-sync-engine-worker",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"lint": "concurrently \"npm run fix:formatting\" \"npm run fix:linting\"  \"npm run check:types\"",
		"check:formatting": "prettier --check . --ignore-path .checkignore",
		"check:linting": "eslint . --ignore-path .checkignore",
		"check:types": "tsc --noEmit",
		"watch:types": "tsc --noEmit --incremental --watch",
		"fix:formatting": "prettier --write . --ignore-path .checkignore",
		"fix:linting": "eslint . --fix --ignore-path .checkignore"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240405.0",
		"@types/lodash": "^4.17.0",
		"@types/uuid": "^9.0.8",
		"typescript": "^5.4.5",
		"wrangler": "^3.50.0"
	},
	"dependencies": {
		"lodash": "^4.17.21",
		"uuid": "^9.0.1"
	}
}
