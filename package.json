{
	"name": "realtime-sync-engine",
	"version": "1.0.0",
	"private": true,
	"workspaces": [
		"client",
		"worker"
	],
	"scripts": {
		"build": "npm run build --workspaces",
		"deploy": "npm run deploy --workspaces",
		"dev": "concurrently \"npm run dev --workspace=client\" \"npm run dev --workspace=worker\"",
		"lint": "npm run lint --workspaces"
	},
	"devDependencies": {
		"@fingertip/eslint-config": "^1.0.8",
		"@fingertip/prettier-config": "^1.0.2",
		"concurrently": "^8.2.2"
	},
	"dependencies": {}
}
